<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:BlndrerGUI.ViewModels"
             x:Class="BlndrerGUI.Views.HeaderDataControl"
             x:DataType="vm:BlndControlViewModel"
             Padding="9">
    
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock Width="130" Margin="0,0,20,0" FontWeight="SemiBold" FontFamily="{StaticResource EnriquetaFont}" Text="EngineType:"/>
            <TextBlock Text="{ Binding BlndFile.Header.mEngineType}" />
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock Width="130" Margin="0,0,20,0" FontWeight="SemiBold" FontFamily="{StaticResource EnriquetaFont}" Text="BinaryBlockType:" />
            <TextBlock Text="{Binding BlndFile.Header.mBinaryBlockType}" />
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock Width="130" Margin="0,0,20,0" FontWeight="SemiBold" FontFamily="{StaticResource EnriquetaFont}" Text="BinaryBlockVersion:" />
            <TextBlock Text="{Binding BlndFile.Header.mBinaryBlockVersion}" />
        </StackPanel>
        
        
        
    </StackPanel>
    
</UserControl>
